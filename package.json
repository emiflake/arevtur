{
  "name": "poe-pricer",
  "version": "4.5.0",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "start-ts": "cp -rT src lib && npx tsc --project tsconfig.json && electron .",
    "test": "cp -rT src lib && npx tsc --project tsconfig.json && electron lib/src/test.js",
    "build-dev": "electron-builder . -c.compression=store",
    "clean-build": "rm -rf dist; electron-builder .",
    "ship": "git push && build --win -p always"
  },
  "author": "mahhov1",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "bs-better-stream": "^1.9.5",
    "electron": "^31.3.1",
    "electron-updater": "^6.2.1",
    "env-paths": "^2.2.0",
    "js-desktop-base": "^5.4.5",
    "xx-element": "^2.0.2"
  },
  "devDependencies": {
    "electron-builder": "^20.44.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mahhov/poe-pricer.git"
  },
  "bugs": {
    "url": "https://github.com/mahhov/poe-pricer/issues"
  },
  "homepage": "https://github.com/mahhov/poe-pricer#readme",
  "description": "",
  "build": {
    "asar": false,
    "artifactName": "${name}-${version}-${os}-${arch}.${ext}",
    "files": [
      "resources",
      "src"
    ],
    "icon": "./resources/icons/fa-dollar-sign-solid-256.png"
  }
}
